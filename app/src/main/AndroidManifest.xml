<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhuliyi.myleakcanary">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SinleInstanceActivity" />
        <activity android:name=".HandlerActivity" />
        <activity android:name=".NoStaticSubClassActivity" />
        <activity android:name=".ThreadActivity" />
        <activity android:name=".ResNoCloseActivity"></activity>
    </application>

</manifest>